{
  "name": "challenge-game-analytics-api",
  "version": "0.0.1",
  "description": "Desafio de análise de logs para seções analíticas de um  jogo de tiro em primeira pessoa",
  "author": "Marcos Vasconcellos",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch -- --inspect-brk=localhost:9229",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config vitest.config.e2e.mjs",
    "test:e2e:watch": "vitest --config vitest.config.e2e.mjs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/client-sqs": "^3.750.0",
    "@aws-sdk/s3-request-presigner": "^3.750.0",
    "@jasonsoft/nestjs-seq": "^2.1.3",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/platform-express": "11.0.10",
    "@nestjs/swagger": "^11.0.4",
    "@prisma/client": "^6.4.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "ioredis": "^5.5.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.2",
    "sqs-consumer": "^11.5.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@rocketseat/eslint-config": "^2.2.2",
    "@swc/core": "^1.10.18",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.5",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.0.6",
    "dotenv": "^16.4.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.2",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  }
}
