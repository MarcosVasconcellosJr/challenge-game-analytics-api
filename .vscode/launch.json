{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "run",
        "start:debug",
        "--",
        "--inspect-brk=localhost:9229"
      ],
      "attachSimplePort": 9229,
      "autoAttachChildProcesses": true,
      "restart": true,
      "sourceMaps": true,
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "preLaunchTask": "CleanDist",
      "skipFiles": ["<node_internals>/**"],
    }
  ]
}